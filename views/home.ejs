<body>
    <header>
        <div class="header-container">
    
        <div class="div-logo">
            <div class = "icon-bar">
                <i class="fas fa-bars"></i>
            </div>
           <a href="/"> <img src="/images/LeMaison.png" alt="logo"> </a>
            <ul>
                <li><a href="#"><i class="fas fa-search"></i></a></li>
                <li><a href="/user/login"><i class="far fa-user"></i></a></li>
                <li><a href="/product/cart"><i class="fas fa-shopping-cart"></i></a></li>
            </ul>
        </div>
    
        <nav class="header-nav">
            <ul>
                <li><a href="/">Home</a></li>
                <li><a href="/product/detail">Productos</a></li>
                <li><a href="/product/new">Nuevo Producto</a></li>
                <li><a href="/product/mod">Modificar Producto</a></li>
            </ul>
        </nav>
        </div>
