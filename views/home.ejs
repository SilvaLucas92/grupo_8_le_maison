    <main>
        <img src="images/1.jpg" alt="banner" class="img-banner">
        <section class="main-section1">
            <h2>Nueva Coleccion</h2>
            <div class="div-art">
            <article class="section1-art1">
                <img src="images/escritorio1.jpg" alt="" >
            </article>
            <article class="section1-art2">
                <img src="images/sillon.jpg" alt="" >
                <img src="images/mesa.jpg" alt="" >    
            </article>
            </div>
        </section>
        <section class="main-section2">
            <h2>Nuestros productos mas comprados</h2>
            <div class="div-art2">
            <article class="section2-art">
                <img src="images/img1-section2.jpg" alt="">
                <div class="art-text">
                    <h3>$8900</h3>
                    <span>Escritorio</span>
                </div>
            </article>
            <article class="section2-art">
                <img src="images/espejo.jpeg" alt="">
                <div class="art-text">
                <h3>$4800</h3>
                <span>Espejo</span>
                </div>
            </article>
            <article class="section2-art">
                <img src="images/estante.jpg" alt="">
                <div class="art-text">
                <h3>$15000</h3>
                <span>Biblioteca</span>
                </div>
            </article>
            <article class="section2-art">
                <img src="images/sillon1.jpg" alt="">
                <div class="art-text">
                <h3>$32000</h3>
                <span>Sillon</span>
                </div>
            </article>
            <article class="section2-art">
                <img src="images/colcha.jpg" alt="">
                <div class="art-text">
                <h3>$3900</h3>
                <span>Alcolchado</span>
                </div>
            </article>  
            <article class="section2-art">
                <img src="images/mesaluz.jpg" alt="">
                <div class="art-text">
                <h3>$24300</h3>
                <span>Mesa Ratona</span>
                </div>
            </article>  
        </div>                                                        
        </section>
        <form action="" method="get" class="form-newsletter">
            <div class="div-form">
                <label>NEWSLETTER</label>
                <input type="text" placeholder="Ingresa tu E-Mail">
                <button type="submit">ENVIAR</button>
            </div>
        </form>
    </main>
    <footer>
        <div class="footer-container">
            <ul class="list-foot">
                <li>
                    <ul class="list-foot2">
                        <h3>Navegacion</h3>
                        <li><a href="/">Home</a></li>
                        <li><a href="/product/detail">Productos</a></li>
